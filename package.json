{
  "name": "ngrx-store-sync",
  "version": "0.2.0",
  "description": "State and storage syncing for @ngrx/store",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmdickson/ngrx-store-sync.git"
  },
  "keywords": [
    "redux",
    "ngrx",
    "store",
    "localstorage",
    "localforage",
    "rxjs"
  ],
  "author": "Cameron Matthews-Dickson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmdickson/ngrx-store-sync/issues"
  },
  "homepage": "https://github.com/cmdickson/ngrx-store-sync#readme",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngrx-store-sync",
    "build_dist": "npm run clean && ng build ngrx-store-sync --configuration production",
    "npm_pack": "cd dist/ngrx-store-sync && npm pack",
    "package": "npm run build_dist && npm run npm_pack",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "build:ci": "npm run clean && npm run build",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 12.18.3",
    "yarn": ">= 1.22.0"
  },
  "dependencies": {
    "@angular/compiler": "~14.2.2",
    "@angular/core": "~14.2.2",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "~14.2.2",
    "@angular/language-service": "~14.2.2",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/store": "^12.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.26.0",
    "husky": "^7.0.1",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^14.0.0",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.7",
    "ts-node": "^9.1.1",
    "typescript": "~4.6.4",
    "zone.js": "~0.11.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix --max-warnings=0"
  }
}
